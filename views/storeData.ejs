<!DOCTYPE html>
<html>
<head>
    <title>Order Data</title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
</head>
<body>
<div class="container">
    <h2>Orders in the database:</h2>
    <ul>
        <% results.forEach(function(r) { %>
        <li>Customer ID: <%= r._id %></li>
        <ul>
            <li>First Name:<%=r.FIRSTNAME %> </li>
            <li>Last Name: <%=r.LASTNAME %> </li>
            <li>Address: <%=r.STREET %> </li>
            <li>City: <%=r.CITY %> </li>
            <li>State: <%=r.STATE %> </li>
            <li>Zip Code: <%=r.ZIP %> </li>
            <li>E-mail Address: <%=r.EMAIL %> </li>
        </ul>
        <br/><br/>
        <% }); %>
    </ul>
</div>

</body>
</html>